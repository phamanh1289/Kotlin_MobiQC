<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="@dimen/_8dp"
    android:background="@drawable/shape_boder_grey_text_view"
    android:focusable="true"
    android:padding="@dimen/_8dp"
    tools:ignore="MissingPrefix">


    <TextView
        android:id="@+id/textView1"
        fontPath="@string/Helvetica_Bold"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_8dp"
        android:layout_marginTop="@dimen/_8dp"
        android:text="@string/check_list_id"
        android:textSize="@dimen/_13dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <TextView
        android:id="@+id/itemCheckList_tvID"
        fontPath="@string/Helvetica_light"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:textSize="@dimen/_13dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView6"
        app:layout_constraintTop_toTopOf="@+id/textView1"
        tools:text="ID"/>

    <TextView
        android:id="@+id/textView2"
        fontPath="@string/Helvetica_Bold"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_8dp"
        android:text="@string/check_list_create"
        android:textSize="@dimen/_13dp"
        app:layout_constraintStart_toStartOf="@+id/textView1"
        app:layout_constraintTop_toBottomOf="@+id/textView1"/>

    <TextView
        android:id="@+id/itemCheckList_tvCreateUpdate"
        fontPath="@string/Helvetica_light"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:textSize="@dimen/_13dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/itemCheckList_tvID"
        app:layout_constraintTop_toTopOf="@+id/textView2"
        tools:text="ID"/>

    <TextView
        android:id="@+id/textView3"
        fontPath="@string/Helvetica_Bold"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_8dp"
        android:text="@string/check_list_type"
        android:textSize="@dimen/_13dp"
        app:layout_constraintStart_toStartOf="@+id/textView2"
        app:layout_constraintTop_toBottomOf="@+id/textView2"/>

    <TextView
        android:id="@+id/itemCheckList_tvType"
        fontPath="@string/Helvetica_light"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:textSize="@dimen/_13dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/itemCheckList_tvCreateUpdate"
        app:layout_constraintTop_toTopOf="@+id/textView3"
        tools:text="ID"/>

    <TextView
        android:id="@+id/textView4"
        fontPath="@string/Helvetica_Bold"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_8dp"
        android:text="@string/check_list_status"
        android:textSize="@dimen/_13dp"
        app:layout_constraintStart_toStartOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/itemCheckList_tvType"/>

    <TextView
        android:id="@+id/itemCheckList_tvStatus"
        fontPath="@string/Helvetica_light"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:textSize="@dimen/_13dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/itemCheckList_tvType"
        app:layout_constraintTop_toTopOf="@+id/textView4"
        tools:text="ID"/>

    <TextView
        android:id="@+id/textView5"
        fontPath="@string/Helvetica_Bold"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_8dp"
        android:text="@string/check_list_last"
        android:textSize="@dimen/_13dp"
        app:layout_constraintStart_toStartOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/textView4"/>

    <TextView
        android:id="@+id/itemCheckList_tvLastUpdate"
        fontPath="@string/Helvetica_light"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:textSize="@dimen/_13dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/itemCheckList_tvStatus"
        app:layout_constraintTop_toTopOf="@+id/textView5"
        tools:text="ID"/>

    <TextView
        android:id="@+id/textView6"
        fontPath="@string/Helvetica_Bold"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_8dp"
        android:text="@string/check_list_user"
        android:textSize="@dimen/_13dp"
        app:layout_constraintStart_toStartOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/textView5"/>

    <TextView
        android:id="@+id/itemCheckList_tvUserUpdate"
        fontPath="@string/Helvetica_light"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:textSize="@dimen/_13dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/itemCheckList_tvLastUpdate"
        app:layout_constraintTop_toTopOf="@+id/textView6"
        tools:text="ID"/>
</android.support.constraint.ConstraintLayout>
